# 基本配置
PORT=8444       # 服务端口号
COOKIE=         # Claude cookie，多个cookie用逗号分隔
CookieArray=    # Cookie数组，多个cookie用逗号分隔
CookieIndex=0   # Cookie索引，从1开始计数
Cookiecounter=3 # Cookie计数器
ProxyPassword=  # 代理密码
Ip=127.0.0.1    # 监听IP地址
rProxy=         # 反向代理地址
api_rProxy=     # API反向代理地址

# 提示词样式配置
PROMPT_STYLE_ENABLED=false                                                                              # 是否启用自定义提示词样式
PROMPT_SYSTEMATIC=0.9                                                                                     # 系统性属性百分比
PROMPT_ANALYTICAL=0.8                                                                                     # 分析性属性百分比
PROMPT_METHODICAL=0.9                                                                                     # 方法性属性百分比
PROMPT_NAME="Recover"                                                                                     # 提示词样式名称
PROMPT_SUMMARY="Deliver comprehensive responses through systematic, analytical, and methodical reasoning" # 提示词样式摘要
PROMPT_TEXT="You are an AI assistant focused on providing comprehensive..."                               # 提示词文本

# 系统设置
RenewAlways=true       # 是否总是更新会话
RetryRegenerate=false  # 是否重试重新生成
PromptExperiments=true # 是否启用提示词实验
SystemExperiments=true # 是否启用系统实验
PreventImperson=true   # 是否防止角色扮演
AllSamples=false       # 是否使用所有样本
NoSamples=false        # 是否不使用样本
StripAssistant=false   # 是否移除助手回复
StripHuman=false       # 是否移除人类输入
PassParams=true        # 是否传递参数
ClearFlags=true        # 是否清除标记
PreserveChats=false    # 是否保留聊天记录
LogMessages=true       # 是否记录消息
FullColon=true         # 是否使用完整冒号
padtxt=1000,1000,15000 # 填充文本配置
xmlPlot=true           # 是否启用XML剧情
SkipRestricted=false   # 是否跳过受限账号
Artifacts=false        # 是否启用工件功能
Superfetch=true        # 是否启用超级获取
